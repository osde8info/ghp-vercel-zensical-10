/*ESQUEMA CDIGO NOVO AGILETV 2025*/
/*agiletv check 27-01-2025*/
#scheme h6, #scheme p{
    color: #fff;
    text-align: center;
}
#scheme h6 {
  font-weight: 600;
  font-size: 25px;
  margin: 0;
  margin-bottom: 10px;
}
#scheme p {
  font-weight: 400;
  margin: 0;
}
#scheme .gradient {
  background: linear-gradient(90deg, #8784cb, #38b9b7, #2acc75);
  background: -webkit-linear-gradient(0deg, #8784cb, #38b9b7, #2acc75);
  background-clip: text;
  -webkit-background-clip: text;
  text-fill-color: transperent;
  -webkit-text-fill-color: transparent;
}
#scheme .category {
  background: #282836;
  border-radius: 12px;
  margin: 10px;
  transition: 0.2s ease-in-out;
  border: 1px solid #282836;
}
#scheme .category > a, #scheme-technologies {
    padding: 30px;
    display: block;
}
#scheme #scheme-technologies > a {
    padding: 0;
}
#scheme #scheme-technologies h6 {
    padding-bottom: 20px;
}
#scheme .category-hover:hover {
  background: #28283620;
  background: conic-gradient(#0e0e1e 0 0) padding-box,
    linear-gradient(120deg, #9f83cc, #7a85cb, #31a5cc, #3fcca3, #1db954)
      border-box;
  border: 1px solid #0000;
}
#scheme .sub-category {
  border-radius: 20px;
  border: 1px solid #161625;
  margin: 10px;
  padding: 5px;
  background-color: #161625;
  transition: 0.2s ease-in-out;
}
#scheme .sub-category:hover {
  background: conic-gradient(#0E0E1E 0 0) padding-box,
    linear-gradient(120deg, #9f83cc, #7a85cb, #31a5cc, #3fcca3, #1db954)
      border-box;
  border: 1px solid #0000;
}
#scheme a:link {
  text-decoration: none;
}

@media only screen and (min-width: 720px) {
  #scheme {
    display: grid;
    grid-template-areas:
      "technologies platform manager"
      "services services services";
    grid-template-columns: 1fr 1fr 1fr;
    gap: 10px;
  }
  #scheme #scheme-technologies {
    grid-area: technologies;
  }
  #scheme #scheme-platform {
    grid-area: platform;
  }
  #scheme #scheme-manager {
    grid-area: manager;
  }
  #scheme #scheme-services {
    grid-area: services;
  }
  #scheme .category {
    margin: 0;
  }
  #scheme #scheme-services a {
    display: flex;
    justify-content: space-around;
    align-items: center;
  }
  #scheme #scheme-services h6 {
    margin-bottom: 0;
  }
  #scheme #scheme-platform a, #scheme #scheme-manager a, #scheme #scheme-services a {
    top: 50%;
    position: relative;
    transform: translateY(-50%);
  }
}

