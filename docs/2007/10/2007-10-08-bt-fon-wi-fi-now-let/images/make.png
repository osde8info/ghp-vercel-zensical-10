/* <![CDATA[ */
/*hide/show contact button on mobile*/
document.querySelector(".hamburger-box").addEventListener("click", function() {
    if (document.documentElement.classList.contains("is-active-menu")) {
        document.querySelectorAll("header .d-inline-flex").forEach(function(el) {
        el.setAttribute('style', 'display:none!important');
        });
    } else {
        document.querySelectorAll("header .d-inline-flex").forEach(function(el) {
        el.setAttribute('style', 'display:inline-flex!important');
        });
    }
});

function customInitOnLoad() {
    
    if (typeof jQuery !== 'undefined') {
        console.log("loaded page");
        jQuery(document).ready(function($) {

          // Your jQuery code here
            $(window).on("load", function () {
                console.log("window loaded");
                console.log("data should be working");
            //add link to blog images
              $("div.elementor-wmidget-pix-blog").each(function () {
                var articles = $(this).find("article");
            
                articles.each(function () {
                    var title =$(this).find("h4");
                  var tag = $(title).find("a");
                  var src = tag.attr("href");
            
                  $(this).wrap(function () {
                    return '<a href="' + src + '"></a>';
                  });
                });
              });
            });
        });
        
        $(document).ready(function () {
          $(".cls-4").css("stroke-width", "20px");
          $(".cls-4").css("stroke", "transparent");
        });
        
        /*SVG product structure interaction*/
       let animationInterval;
        let currentIndex = 0;
        
        function handleHover(event) {
          const $parentG = $(this);
          const $link = $(this).parent("a");
        
          // Toggle classes based on event type
          if (event.type === "mouseenter") {
            if ($parentG.is("#products > a > g")) {
              $parentG.addClass("active-title");
              $("g[data-name='transparencia']").removeClass("transparency");
              $("#transparencia").removeClass("transparency");
              $parentG.find("g[data-name='transparencia']").addClass("transparency");
              $parentG.find("#transparencia").addClass("transparency");
              clearInterval(animationInterval); // Stop the animation
            }
          } else if (event.type === "mouseleave") {
            if ($parentG.is("#products > a > g")) {
              $parentG.removeClass("active-title");
              $("g[data-name='transparencia']").removeClass("transparency");
              $parentG.find("g[data-name='transparencia']").removeClass("transparency");
              $parentG.find("#transparencia").removeClass("transparency");
              highlightNextElement();
              startAnimation(); // Resume the animation
            }
          }
        }
        
        function highlightNextElement() {
          const $elements = $("#products > a > g");
        
          // Move to the next element in reverse order
          currentIndex--;
          if (currentIndex < 0) {
            currentIndex = $elements.length - 1;
          }
        
          // Highlight the current element
          $elements.removeClass("active-title");
          $("g[data-name='transparencia']").removeClass("transparency");
          $("#transparencia").removeClass("transparency");
          $elements.eq(currentIndex).addClass("active-title");
          $elements
            .eq(currentIndex)
            .find("g[data-name='transparencia']")
            .addClass("transparency");
          $elements.eq(currentIndex).find("#transparencia").addClass("transparency");
        }
        
        function startAnimation() {
          animationInterval = setInterval(() => {
            highlightNextElement();
          }, 2000); // Adjust the interval as needed
        }
        
        $(document).ready(function () {
          // Bind hover event handler
          $("#products > a > g").hover(handleHover);
          highlightNextElement();
          // Start the initial animation
          startAnimation();
        });
        
        // Unbind hover event handler and clear interval when navigating away from the page
        $(window).on("unload", function () {
          $("#products > a > g").off("mouseenter mouseleave", handleHover);
          clearInterval(animationInterval);
        });
        
        $(document).ready(function () {
          $(".product").on("click", function () {
            var $clickedProduct = $(this);
        
            // Check if the clicked product's info is already visible
            var isAlreadyOpen = $clickedProduct.find(".info").is(":visible");
        
            // Close all other elements
            $(".product .info").slideUp();
            $(".product .dropdown").removeClass("dropdown-active");
            $(".product .linha").removeClass("oculto");
        
            // If the clicked product's info was already open, do nothing
            if (!isAlreadyOpen) {
              // Toggle the elements within the clicked product
              $clickedProduct.find(".info").slideToggle();
              $clickedProduct.find(".dropdown").toggleClass("dropdown-active");
              $clickedProduct.find(".linha").toggleClass("oculto");
            }
          });
        });

    } else {
      // jQuery is not loaded, handle the situation gracefully
      console.error('jQuery is not loaded. Your code may not work as expected.');
    }
}

// Attach the function to the window.onload event
window.onload = customInitOnLoad;

// Display thumbnail after video plays
document.addEventListener("DOMContentLoaded", function () {
    const wrappers = document.querySelectorAll(".pix-video");

    wrappers.forEach(wrapper => {
        const video = wrapper.querySelector("video");
        if (!video) return;

        video.addEventListener("ended", function () {
            console.log("video finished");
            wrapper.classList.remove("video-start");
            wrapper.classList.remove("video-active");
            video.currentTime = 0;
            video.pause();
        });
    });
});
